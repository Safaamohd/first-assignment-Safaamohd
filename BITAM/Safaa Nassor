    QN 4
    public class PersonalInfo {
         public static void main(String[] args) {
        // Displaying name, address, and telephone number
        System.out.println("Name: Your Name");
        System.out.println("Address: Your Address");

        // Adding a blank line
        System.out.println();

        System.out.println("Telephone Number: Your Phone Number");
    }
}

QN 5
    public class Error {
    public static void main(String[] args){
        System.out.println("I want this program to compile");
    }
}

QN 6
  public class InitialsDisplay {
    public static void main(String[] args) {
        displayS();
        System.out.println(); // Add a blank line between initials
        displayN();
    }

    // Method to display the letter 'S'
    private static void displayS() {
        System.out.println("  *****");
        System.out.println(" *");
        System.out.println("  *****");
        System.out.println("       *");
        System.out.println("  *****");
    }

    // Method to display the letter 'N'
    private static void displayN() {
        System.out.println(" *     *");
        System.out.println(" **    *");
        System.out.println(" * *   *");
        System.out.println(" *  *  *");
        System.out.println(" *   **");
        System.out.println(" *    *");
    }
}
 





